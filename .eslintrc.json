{
    "extends": ["eslint:recommended", "google", "plugin:jasmine/recommended"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "jasmine": true,
        "es6": true
    },
    "plugins": [
        "jasmine"
    ],
    "rules": {
        "jasmine/no-spec-dupes": [1, "branch"],
        "jasmine/no-suite-dupes": [1, "branch"]
    }
}
